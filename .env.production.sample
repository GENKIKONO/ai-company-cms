# Supabase Configuration (REQUIRED)
# Supabase project URL (Dashboard > Settings > General > Project URL)
# REQUIRED
SUPABASE_URL=https://your-project.supabase.co

# Supabase service role key (Dashboard > Settings > API > service_role secret)
# REQUIRED
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Supabase anonymous key (Dashboard > Settings > API > anon public)
# REQUIRED
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Public Supabase URL for client-side (same as SUPABASE_URL)
# REQUIRED
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co


# Application URLs (REQUIRED)
# Application base URL (your production domain)
# REQUIRED
NEXT_PUBLIC_APP_URL=https://aiohub.jp

# Site URL for API calls (same as NEXT_PUBLIC_APP_URL)
# REQUIRED
NEXT_PUBLIC_SITE_URL=https://aiohub.jp


# Admin Settings (OPTIONAL)
# Admin email for system notifications
ADMIN_EMAIL=admin@aiohub.jp

# Admin operations password
ADMIN_OPS_PASSWORD=your-admin-password

# Token for on-demand ISR revalidation API
REVALIDATE_TOKEN=your-revalidate-token


